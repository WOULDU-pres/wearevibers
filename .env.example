# WeAreVibers Environment Variables Example
# Copy this file to .env and fill in your actual values

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key
VITE_SITE_URL=http://localhost:8080

# Supabase API Keys (Backend only - never expose in frontend)
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Database Connection (Backend only)
SUPABASE_DB_HOST=your-db-host
SUPABASE_DB_PORT=5432
SUPABASE_DB_NAME=postgres
SUPABASE_DB_PASSWORD=your-password

# OAuth Configuration
VITE_GOOGLE_CLIENT_ID=your-google-client-id

# Error Monitoring (Sentry)
VITE_SENTRY_DSN=your-sentry-dsn
VITE_SENTRY_ORG=your-sentry-org
VITE_SENTRY_PROJECT=your-sentry-project
SENTRY_AUTH_TOKEN=your-sentry-auth-token

# Deployment (Vercel)
VERCEL_TOKEN=your-vercel-token
VERCEL_ORG_ID=your-org-id
VERCEL_PROJECT_ID=your-project-id

# Performance Monitoring
VITE_WEB_VITALS_ENABLED=true
VITE_PERFORMANCE_API_URL=your-performance-api-url

# Feature Flags
VITE_FEATURE_FLAG_BETA=false
VITE_FEATURE_FLAG_ANALYTICS=true
VITE_FEATURE_FLAG_DEBUG=false

# CDN Configuration
VITE_CDN_URL=https://your-cdn-url
VITE_ASSETS_URL=https://your-assets-url